{
  "name": "material-ui-datetime-range-picker",
  "private": true,
  "author": "JK Slyby",
  "version": "1.0.0",
  "description": "React Datetime Range Picker Component that Implements Google's Material Design Via Material-UI.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "datetime",
    "datetime-picker",
    "datetime-range-picker",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkslyby/material-ui-datetime-range-picker.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkslyby/material-ui-datetime-range-picker/issues"
  },
  "homepage": "http://material-ui.com/",
  "scripts": {
    "watch": "npm run build:copy-files && npm run build:babel:watch",
    "build": "npm run build:babel && npm run build:copy-files && npm run build:es",
    "build:es": "npm run build:es-index",
    "build:es-index": " cross-env BABEL_ENV=es babel ./src/index.js --out-file ./build/index.es.js",
    "build:babel": "cross-env NODE_ENV=test babel ./src --out-dir ./build --ignore spec.js",
    "build:babel:watch": "cross-env NODE_ENV=test babel --watch ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "lint": "eslint src --quiet --cache && echo \"eslint: no lint errors\"",
    "lint:find-rules": "eslint-find-rules -u .eslintrc.js",
    "prebuild": "npm run clean:build",
    "postpublish": "npm run build && cd build && npm publish"
  },
  "peerDependencies": {
    "react": "^15.4.0 || ^16.0.0-beta.5",
    "react-dom": "^15.4.0 || ^16.0.0-beta.5"
  },
  "dependencies": {
    "@material-ui/core": "4.4.3",
    "@material-ui/icons": "4.4.3",
    "@material-ui/styles": "4.4.3",
    "babel-runtime": "^6.23.0",
    "inline-style-prefixer": "^3.0.2",
    "keycode": "^2.1.8",
    "lodash.find": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.sortby": "4.7.0",
    "lodash.throttle": "^4.1.1",
    "parse-num": "^1.0.0",
    "prop-types": "^15.5.7",
    "react-addons-update": "^15.6.2",
    "react-event-listener": "^0.4.5",
    "react-transition-group": "^1.1.2",
    "recompose": "0.24.0",
    "simple-assign": "^0.1.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^3.0.4",
    "create-react-class": "^15.5.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.1",
    "eslint": "^4.18.2",
    "eslint-find-rules": "^3.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-material-ui": "./packages/eslint-plugin-material-ui",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^7.0.0",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "karma": "^4.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "phantomjs-prebuilt": "^2.1.16",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-test-renderer": "^15.5.0",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "sinon": "^3.0.0",
    "webpack": "^3.12.0"
  }
}
